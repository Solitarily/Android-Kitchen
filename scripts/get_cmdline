if [ ! -d WORKING_* ]
then
  echo " 未发现工作文件夹！"
  exit 0
fi

cd WORKING_*

if [ ! -e boot.img ]
then
  echo " 未发现 boot.img!"
  exit 0
fi

cmd_line=`od -A n --strings -j 64 -N 512 boot.img`

echo $cmd_line
